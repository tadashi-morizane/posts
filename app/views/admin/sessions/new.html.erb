<h1>管理者画面にログイン</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@admin,url: admin_login_path) do |f| %>

     
      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      
      
      <%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
      
      
    <% end %>
  </div>
</div>
